# AWS Credentials
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_KEY
AWS_DEFAULT_REGION=ap-south-1
AWS_SESSION_TOKEN=YOUR_AWS_SESSION_TOKEN  # If using temporary credentials

# S3 Buckets
# GPS Data
S3_TRAIN_GPS_BUCKET=hybrid-eta-train-gps-data
S3_TEST_GPS_BUCKET=hybrid-eta-test-gps-data
# Models and Coefficients
S3_MODEL_BUCKET=hybrid-eta-models
S3_DFT_COEFFICIENTS_BUCKET=hybrid-eta-dft-coefficients
S3_WEATHER_CACHE_BUCKET=hybrid-eta-weather-cache

# ArangoDB Configuration
ARANGO_HOST=your-arangodb-host
ARANGO_PORT=8529
ARANGO_USER=hybrid_eta_user
ARANGO_PASSWORD=your_arangodb_password
ARANGO_GRAPH_DB=hybrid_eta_graph
ARANDOB_GRAPH_NAME=bus_network_graph

# OpenWeatherMap API
OPENWEATHER_API_KEY=your_openweather_api_key
OPENWEATHER_BASE_URL=https://api.openweathermap.org/data/2.5

# Model Storage Paths
MODEL_BASE_PATH=/models
MST_AV_MODEL_PATH=${MODEL_BASE_PATH}/mst_av
GDRN_DFT_MODEL_PATH=${MODEL_BASE_PATH}/gdrm_dft
KNN_MODEL_PATH=${MODEL_BASE_PATH}/knn
FENN_MODEL_PATH=${MODEL_BASE_PATH}/fenn
MGCN_MODEL_PATH=${MODEL_BASE_PATH}/mgcn
HYBRID_MODEL_PATH=${MODEL_BASE_PATH}/hybrid

# DFT Configuration
DFT_CUTOFF_AMPLITUDE=0.1  # ε_g in the algorithm
DFT_TIME_STEP=300  # δ_g in seconds (5 minutes)
DFT_WINDOW_SIZE=288  # Number of time steps (24 hours at 5-min intervals)

# KNN Configuration
KNN_WINDOW_SIZE=12  # k in the algorithm
KNN_MAX_SPEED=100  # V_max in km/h
KNN_MIN_SPEED=0.1  # ε in km/h
KNN_REGULARIZATION=0.01  # λ_k

# FENN Configuration
FENN_TIME_FEATURES=4  # b in the algorithm
FENN_HIDDEN_LAYERS=3  # l_f
FENN_TIME_STEP=300  # δ_n in seconds (5 minutes)

# MGCN Configuration
MGCN_NUM_LAYERS=3  # L in the algorithm
MGCN_HIDDEN_DIM=64
MGCN_DROPOUT=0.2
MGCN_LAMBDA=0.5  # λ_m for loss balancing

# Hybrid Model Configuration
HYBRID_TOP_MODELS=3  # n_m in the algorithm
HYBRID_WEIGHT_SMOOTHING=0.1  # For exponential moving average of weights

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=/var/log/hybrid_eta/hybrid_eta.log

# Feature Flags
ENABLE_WEATHER_UPDATES=true
ENABLE_REALTIME_UPDATES=true
ENABLE_MODEL_RETRAINING=false

# Performance Tuning
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30  # seconds
CACHE_TTL=300  # 5 minutes cache TTL for weather data

# Monitoring
PROMETHEUS_ENDPOINT=localhost:9090
GRAFANA_DASHBOARD_URL=http://localhost:3000/d/hybrid-eta

# Security
JWT_SECRET=your_jwt_secret_key_here
CORS_ORIGINS=*  # In production, replace with specific origins

# Environment
NODE_ENV=development  # or production, staging
DEBUG=true  # Set to false in production